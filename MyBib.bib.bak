@Article{Ordonez1995Antipassive,
  Title                    = {The antipassive in Jacaltec: a last resort strategy},
  Author                   = {Ord{\'o}{\~n}ez, Francisco},
  Journal                  = {CatWPL},
  Year                     = {1995},
  Number                   = {2},
  Pages                    = {329-343},
  Volume                   = {4},

  Abstract                 = {This paper gives reasons to recover the notion of last resort, found in Chomsky (1991) by examining two uses of the antipassive suffix -ni in Jacaltec, an ergative Mayan language. This suffix is inserted as a last resort where UG makes the assignment of case to the object (transitive aspectless embedded clauses) and the extraction of the subject with ergative agreement (aspectual main clauses) otherwise impossible. Therefore, this notion cannot be restricted to instances of movement, as in Chomsky (1994). Rather, it must also include lexical insertion of affixal material in the deriva- tion, as in the broader view found in Chomsky (1991). },
